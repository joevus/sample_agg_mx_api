<p id="notice"><%= notice %></p>

<h1>Accounts</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @accounts.each do |account| %>
      <tr>
        <td>guid: <%= account.guid %></td>
      </tr>
      <tr>
        <td>balance: <%= account.balance %></td>
      </tr>
    <% end %>
  </tbody>
</table>
