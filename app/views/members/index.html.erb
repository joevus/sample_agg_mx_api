<p id="notice"><%= notice %></p>

<h1>Members</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @members.each do |member| %>
      <tr>
        <td>id: <%= member.id %></td>
      </tr>
      <tr>
        <td>mx_id: <%= member.mx_id %></td>
      </tr>
      <tr>
        <td>name: <%= member.name %></td>
      </tr>
      <tr>
        <td>institution_code: <%= member.institution_code %></td>
      </tr>
      <tr>
        <td>guid: <%= member.guid %></td>
      </tr> 
      <tr>
        <td><%= link_to 'Show', user_member_path(member.user, member) %></td>
        <td><%= link_to 'Edit', edit_user_member_path(member.user, member) %></td>
        <td><%= link_to 'Destroy', user_member_path(member.user, member), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Member', new_user_member_path %>
